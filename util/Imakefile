#define CONFIG_TOP
#include "../Config.h"
#undef CONFIG_TOP

GEN_SRCS = \
error.cc \
text.cc \
arithmetic.cc \
command.cc \
file.cc

PROTECTED_SRCS =

SRCS = $(GEN_SRCS) $(PROTECTED_SRCS) $(EXTRA_SRCS)

OBJS = $(SRCS:.cc=.o)

GEN_HDRS = \
error.h \
text.h \
arithmetic.h \
command.h \
file.h \
search.h \
handleSet.h \
writeSet.h \
visit.h

PROTECTED_HDRS =

INCLUDE_FILES = $(GEN_HDRS) $(PROTECTED_HDRS)

INCLUDES = -I.. -I../src -I../objects

CXXOPTIONS = $(CXX_OBJECT_OPTIONS)

TGTPATH = ../$(OBJDIR)

vpath %.o $(TGTPATH)

NormalLibraryTarget(util,$(OBJS))
DependTarget()


clean ::
	rm -f *.tmp*

