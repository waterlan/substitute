#define CONFIG_TOP
#include "Config.h"
#undef CONFIG_TOP

#define IHaveSubdirs
SUBDIRS = src util objects
MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))

LOCAL_LIBRARIES = \
./src/libsrc.a \
./util/libutil.a \
./objects/libobjects.a \
./src/libsrc.a \
./util/libutil.a \
./objects/libobjects.a
DEPLIBS=$(LOCAL_LIBRARIES)

BINDIR = ${HOME}/bin

GEN_SRCS = \
main.cc

PROTECTED_SRCS = \
Usage.cc \
Driver.cc

SRCS = $(GEN_SRCS) $(PROTECTED_SRCS) $(EXTRA_SRCS)

OBJS = $(SRCS:.cc=.o)

GEN_HDRS = \
VERSION.h \
std_macro.h \
structures.h

PROTECTED_HDRS = \
Config.h \
Constants.h \
Usage.h \
Driver.h

INCLUDE_FILES = $(GEN_HDRS) $(PROTECTED_HDRS)

INCLUDES = -I. -Isrc -Iutil -Iobjects

CXXOPTIONS = $(CXX_TOPLEVEL_OPTIONS)

TGTPATH = $(OBJDIR)

vpath %.o $(TGTPATH)

#if defined CppManTarget
#undef CppManTarget
#endif
#define CppManTarget(name, defs)

#if !defined ComplexCplusplusProgramTarget
#define ComplexCplusplusProgramTarget(x) ComplexProgramTarget(x)
#endif

ComplexCplusplusProgramTarget(S)

clean ::
	rm -f *.tmp*

#define CONFIG_BOT
#include "Config.h"
#undef CONFIG_BOT

