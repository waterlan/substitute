INCLUDES = -I.. -I../src -I../util

CXXOPTIONS = $(CXX_OBJECT_OPTIONS)

TGTPATH = ../$(OBJDIR)

vpath %.o $(TGTPATH)

NormalLibraryTarget(objects,$(OBJS))
DependTarget()


