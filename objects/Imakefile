OBJS = $(SRCS:.cc=.o)

GEN_HDRS = \
search_rangeset.h \
search_range.h \
search_match.h \
action.h \
actionset.h \
nameset.h \
intset.h \
intsets.h \
objset.h

PROTECTED_HDRS =

